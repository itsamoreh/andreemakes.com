		"layout": {
			"contentSize": "750px",
			"wideSize": "1500px"
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Light",
					"slug": "light",
					"shadow": "0px 8px 20px rgb(255 255 255 / 16%), 0px 8px 20px rgb(0 0 0 / 6%);"
				},
				{
					"name": "Normal",
					"slug": "normal",
					"shadow": "0px 4px 8px rgb(255 255 255 / 16%), 0px 4px 8px rgb(0 0 0 / 6%);"
				},
				{
					"name": "Hard",
					"slug": "hard",
					"shadow": "0px 2px 4px rgb(255 255 255 / 32%), 0px 2px 4px rgb(0 0 0 / 12%);"
				}
			]
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "Extra Extra Small",
					"size": "8px",
					"slug": "xx-small"
				},
				{
					"name": "Extra Small",
					"size": "16px",
					"slug": "x-small"
				},
				{
					"name": "Small",
					"size": "24px",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "32px",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "48px",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "64px",
					"slug": "x-large"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textDecoration": false,
			"fontFamilies": [
				{
					"fontFamily": "Bariol Serif, serif",
					"name": "Bariol Serif",
					"slug": "serif",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Bariol Serif",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/bariol_serif_regular-webfont.woff2"]
						}
					]
				},
				{
					"fontFamily": "Bariol Serif, serif",
					"name": "Bariol Serif Bold",
					"slug": "serif-bold",
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Bariol Serif",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/bariol_serif_bold-webfont.woff2"]
						}
					]
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"name": "Caption",
					"size": "1rem",
					"slug": "caption"
				},
				{
					"name": "Body",
					"size": "1.25rem",
					"slug": "body",
					"fluid": false
				},
				{
					"name": "H5",
					"size": "1.563rem",
					"slug": "h5"
				},
				{
					"name": "H4",
					"size": "1.953rem",
					"slug": "h4"
				},
				{
					"name": "H3",
					"size": "2.441rem",
					"slug": "h3"
				},
				{
					"name": "H2",
					"size": "3.052rem",
					"slug": "h2"
				},
				{
					"name": "H1",
					"size": "3.815rem",
					"slug": "h1"
				},
				{
					"name": "Display",
					"size": "4.768rem",
					"slug": "display"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--black)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--serif)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"fontSize": "var(--wp--preset--font-size--body)"
				},
				"border": {
					"radius": "var(--wp--custom--border-radius--small)",
					"width": "2px",
					"style": "solid",
					"color": "transparent"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--xx-small)",
						"right": "var(--wp--preset--spacing--x-small)",
						"bottom": "var(--wp--preset--spacing--xx-small)",
						"left": "var(--wp--preset--spacing--x-small)"
					}
				},
				"css": "transition: transform 0.1s ease; &:hover { transform: scale(1.05); }",
				"variations": {
					"outline": {
						"border": {
							"color": "currentColor",
							"style": "solid",
							"width": "2px"
						},
						"spacing": {
							"padding": {
								"top": "var(--wp--preset--spacing--xx-small)",
								"right": "var(--wp--preset--spacing--x-small)",
								"bottom": "var(--wp--preset--spacing--xx-small)",
								"left": "var(--wp--preset--spacing--x-small)"
							}
						}
					}
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--custom--spacing--gap)"
				}
			},
			"core/code": {
				"border": {
					"radius": "var(--wp--custom--border-radius--small)"
				},
				"color": {
					"background": "var(--wp--preset--color--black)",
					"text": "var(--wp--preset--color--white)"
				},
				"css": "& code {overflow-wrap: normal; overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"spacing": {
					"padding": "var(--wp--custom--spacing--gap)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--body)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-5)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-5)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-5)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-5)"
				}
			},
			"core/comments": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--x-large)"
					}
				}
			},
			"core/comments-title": {
				"spacing": {
					"margin": {
						"top": "var(--wp--custom--spacing--gap)",
						"bottom": "var(--wp--custom--spacing--gap)"
					}
				}
			},
			"core/cover": {
				"spacing": {
					"padding": "var(--wp--custom--spacing--gap)"
				},
				"border": {
					"radius": "var(--wp--custom--border-radius--small)"
				}
			},
			"core/details": {
				"css": "& ::marker {color: var(--wp--preset--color--pink);}"
			},
			"core/file": {
				"elements": {
					"button": {
						"color": {
							"background": "var(--wp--preset--color--black)",
							"text": "var(--wp--preset--color--white)"
						},
						"typography": {
							"fontFamily": "var(--wp--preset--font-family--serif)",
							"fontWeight": "var(--wp--custom--font-weight--bold)",
							"fontSize": "var(--wp--preset--font-size--body)"
						},
						"border": {
							"radius": "var(--wp--custom--border-radius--small)",
							"width": "2px",
							"style": "solid",
							"color": "transparent"
						},
						"spacing": {
							"padding": {
								"top": "var(--wp--preset--spacing--xx-small)",
								"right": "var(--wp--preset--spacing--x-small)",
								"bottom": "var(--wp--preset--spacing--xx-small)",
								"left": "var(--wp--preset--spacing--x-small)"
							}
						}
					}
				}
			},
			"core/image": {
				"border": {
					"radius": "var(--wp--custom--border-radius--small)"
				}
			},
			"core/list": {
				"spacing": {
					"padding": {
						"left": "var(--wp--custom--spacing--gap)"
					}
				},
				"css": "> ::marker {color: var(--wp--preset--color--pink);} li {color: var(--wp--preset--color--black);}"
			},
			"core/media-text": {
				"css": "& .wp-block-media-text__media img {border-radius: var(--wp--custom--border-radius--small);}"
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-1)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"core/post-comments-form": {
				"css": "& input[type=submit] {width: auto;}"
			},
			"core/post-template": {
				"css": "& .wp-block-post:not(:last-of-type) {margin-bottom: var(--wp--preset--spacing--medium);}"
			},
			"core/preformatted": {
				"border": {
					"radius": "var(--wp--custom--border-radius--small)"
				},
				"color": {
					"background": "var(--wp--preset--color--black)",
					"text": "var(--wp--preset--color--white)"
				},
				"css": "{overflow-wrap: normal; overflow-x: scroll; tab-size: 4; white-space: pre !important;}",
				"spacing": {
					"padding": "var(--wp--custom--spacing--gap)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--body)"
				}
			},
			"core/pullquote": {
				"border": {
					"radius": "var(--wp--custom--border-radius--small)"
				},
				"spacing": {
					"padding": "var(--wp--custom--spacing--gap)"
				},
				"color": {
					"background": "var(--wp--preset--color--blue)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-4)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontWeight": "var(--wp--custom--font-weight--regular)",
							"fontSize": "var(--wp--preset--font-size--body)"
						}
					}
				},
				"css": "> blockquote p {margin-top: 0; margin-bottom: var(--wp--preset--spacing--xx-small);}"
			},
			"core/query": {
				"css": "& .wp-block-post-title { font-size: var(--wp--preset--font-size--h-2); }"
			},
			"core/quote": {
				"border": {
					"radius": "var(--wp--custom--border-radius--small)"
				},
				"spacing": {
					"padding": "var(--wp--custom--spacing--gap)"
				},
				"color": {
					"background": "var(--wp--preset--color--blue)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-5)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				},
				"variations": {
					"plain": {
						"spacing": {
							"padding": {
								"left": "0"
							}
						},
						"color": {
							"background": "transparent"
						}
					}
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--caption)",
							"fontWeight": "var(--wp--custom--font-weight--regular)"
						}
					}
				},
				"css": "> p {margin-top: 0; margin-bottom: var(--wp--preset--spacing--xx-small);}"
			},
			"core/separator": {
				"border": {
					"radius": "0",
					"width": "0"
				},
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-2)"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--h-2)"
				}
			},
			"core/social-links": {
				"css": ".is-style-outline .wp-block-social-link {background-color: transparent;border: 1px solid currentColor;}",
				"spacing": {
					"blockGap": "8px"
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"top": "0 !important"
					}
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0px"
					}
				}
			},
			"core/video": {
				"css": "& video {border-radius: var(--wp--custom--border-radius--small);}"
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
