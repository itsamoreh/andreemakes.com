		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--serif)",
			"fontSize": "var(--wp--preset--font-size--body)",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "var(--wp--custom--line-height--130)"
