				{
					"color": "#FFC8DD",
					"name": "Pink",
					"slug": "pink"
				},
				{
					"color": "#BDE0FE",
					"name": "Blue",
					"slug": "blue"
				},
				{
					"color": "#FAFDFF",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#495159",
					"name": "Charcoal",
					"slug": "charcoal"
				},
				{
					"color": "#1C1F22",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "transparent",
					"name": "Transparent",
					"slug": "transparent"
				}
